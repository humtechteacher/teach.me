{
    "regions": ["iad1"],
    "version": 2,
    "installCommand": "npm install --production=false",
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "dist/public"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "dist/public/$1"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "memory": 1024,
        "maxDuration": 60
      }
    }
  }